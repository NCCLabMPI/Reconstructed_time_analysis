{
  "visit": "1",
  "task": "prp",
  "session": "1",
  "data_type": "eyetrack",
  "preprocessing_steps": [
    "dilation_speed_rejection",
    "trend_line_departure",
    "blinks_interpolation",
    "extract_blinks",
    "extract_saccades",
    "extract_fixation",
    "epochs",
    "remove_bad_epochs",
    "discard_bad_subjects"
  ],
  "events_of_interest": ["vis_onset*"],
  "discard_bad_subjects": {
    "bad_trials_threshold": 0.5,
    "nan_threshold": 0.5
  },
  "trend_line_departure": {
    "eyes": ["left", "right"],
    "threshold_factor": 4,
    "window_length_s": 0.5,
    "polyorder": 3,
    "n_iter": 4
  },
  "blinks_interpolation": {
    "buffer": [0.05, 0.2],
    "interpolate_gaze": true
  },
  "dilation_speed_rejection": {
    "threshold_factor": 5,
    "eyes": ["left", "right"]
  },
  "set_pupil_nans": {
    "eyes": ["left", "right"]
  },
  "interpolate_pupil": {
    "eyes": ["left", "right"],
    "kind": "linear"
  },
  "extract_blinks": {
    "channels": ["L", "R"],
    "description": "blink"
  },
  "extract_saccades": {
    "channels": ["L", "R"],
    "description": "saccade"
  },
  "extract_fixation": {
    "channels": ["L", "R"],
    "description": "fixation"
  },
  "remove_bad_epochs": {
    "channels": ["L", "R"],
    "nan_proportion_thresh": 0.5
  },
  "epochs": {
    "visual_onset": {
      "picks": "all",
      "tmin": -1,
      "tmax": 3.5,
      "baseline": null,
      "events_of_interest": [
          "vis_onset"
      ],
      "reject_by_annotation": false,
      "metadata_column": ["event type", "task relevance", "duration", "category", "orientation", "identity",
          "SOA", "lock", "pitch"]
    }
  }
}